#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'python-glanceclient==2012.2','console_scripts','glance'
__requires__ = 'python-glanceclient==2012.2'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('python-glanceclient==2012.2', 'console_scripts', 'glance')()
    )
